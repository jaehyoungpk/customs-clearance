# 데이터베이스 설정
MYSQL_ROOT_PASSWORD=your-secure-root-password
MYSQL_PASSWORD=your-secure-mysql-password
POSTGRES_PASSWORD=your-secure-postgres-password

# JWT 보안 설정
JWT_SECRET=your-very-long-and-secure-jwt-secret-key-here-at-least-256-bits

# OpenAI API 설정
OPENAI_API_KEY=sk-your-openai-api-key-here

# Azure OCR 설정
AZURE_FORM_RECOGNIZER_ENDPOINT=https://your-region.cognitiveservices.azure.com/
AZURE_FORM_RECOGNIZER_KEY=your-azure-form-recognizer-key

# Railway 특정 환경변수 (프로덕션에서만 사용)
RAILWAY_ENVIRONMENT=production
RAILWAY_PROJECT_NAME=customs-clearance

# 서비스 URL 설정 (프로덕션)
BACKEND_URL=https://customs-backend-production.up.railway.app
AI_GATEWAY_URL=https://customs-ai-gateway-production.up.railway.app
CHATBOT_URL=https://customs-chatbot-fastapi-production.up.railway.app
OCR_URL=https://customs-ocr-production.up.railway.app
REPORT_URL=https://customs-report-production.up.railway.app
HSCODE_URL=https://customs-hscode-production.up.railway.app

# 데이터베이스 URL (Railway에서 자동 생성됨)
DATABASE_URL=mysql://customs_user:${MYSQL_PASSWORD}@mysql.railway.internal:3306/customs_clearance
POSTGRES_URL=postgresql://chatbot_user:${POSTGRES_PASSWORD}@postgres.railway.internal:5432/conversations
REDIS_URL=redis://redis.railway.internal:6379