{
  "name": "customs-clearance-frontend",
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "NEXT_PUBLIC_API_URL": "@api_url",
    "NEXT_PUBLIC_ENVIRONMENT": "production"
  },
  "functions": {
    "app/**": {
      "maxDuration": 30
    }
  }
}